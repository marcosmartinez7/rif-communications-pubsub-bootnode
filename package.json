{
  "name": "@rsksmart/rif-communications-pubsub-node",
  "version": "0.1.0-dev",
  "description": "Node using the PubSub module",
  "keywords": [
    "RIF",
    "Communications",
    "PubSub"
  ],
  "homepage": "https://github.com/rsksmart/rif-communications-pubsub-node#readme",
  "bugs": {
    "url": "https://github.com/rsksmart/rif-communications-pubsub-node/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Vojtech Simetka",
    "email": "vojtech@simetka.cz"
  },
  "files": [
    "config",
    "dist",
    "lib",
    "types"
  ],
  "main": "lib/index.js",
  "repository": "https://github.com/rsksmart/rif-communications-pubsub-node",
  "scripts": {
    "compile": "tasegir compile",
    "compile:watch": "tasegir compile -- --watch",
    "types-check": "tasegir types-check",
    "exec": "tasegir run --watch ./src/index.ts",
    "api-server": "tasegir run --watch ./src/api-server.ts",
    "api-client": "tasegir run --watch ./src/api-client.ts ",
    "create-key": "tasegir run --watch ./src/key-utils.ts ",
    "lint": "tasegir lint",
    "release": "tasegir release",
    "build": "tasegir build",
    "tasegir:link": "ln -s ./node_modules/tasegir/src/config/tsconfig.json ./tsconfig.json && ln -s ./node_modules/tasegir/src/config/eslintrc.js ./.eslintrc.js"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.5",
    "@rsksmart/rif-communications-pubsub": "0.1.0-dev.9",
    "@types/asn1js": "0.0.2",
    "@types/node-forge": "^0.9.5",
    "@types/pkijs": "0.0.5",
    "asn1js": "^2.0.26",
    "chalk": "^4.1.0",
    "config": "^3.3.1",
    "grpc": "^1.24.3",
    "karma": "^5.2.2",
    "key-encoder": "^2.0.3",
    "libp2p": "^0.28.10",
    "libp2p-crypto-secp256k1": "^0.4.3",
    "node-forge": "^0.9.2",
    "node-webcrypto-ossl": "^2.1.2",
    "peer-id": "^0.14.0",
    "pkijs": "^2.1.90"
  },
  "devDependencies": {
    "@types/config": "0.0.36",
    "tasegir": "^1.8.0"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "types": "types/index.d.ts",
  "contributors": [
    "Vojtech Simetka <vojtech@simetka.cz>",
    "Raul Laprida <raul@iovlabs.org>"
  ]
}
